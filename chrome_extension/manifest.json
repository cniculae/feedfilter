{
  "manifest_version": 2,

  "name": "FeedFilter",
  "description": "This extension filters your facebook feed",
  "version": "1.0",
  "browser_action": {
    "default_icon": {
      "256": "icons/icon256x256.png",
      "128": "icons/icon_128x128.png"
    },
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
